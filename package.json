    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.9",
    "pg": "^8.14.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "tedious": "^18.6.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "uuid": "^11.0.3"
