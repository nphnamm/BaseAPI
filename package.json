    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.0",
    "@types/node-cron": "^3.0.11",
    "@types/validator": "^13.7.17",
