{
  "name": "quizzlearn-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon app.ts",
    "dev": "ts-node-dev --respawn --transpile-only app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:postgres": "cross-env NODE_ENV=development nodemon -r dotenv/config app.ts dotenv_config_path=.env.postgres",
    "start:mssql": "cross-env NODE_ENV=local nodemon -r dotenv/config app.ts dotenv_config_path=.env.mssql",
    "start:pc": "cross-env NODE_ENV=local nodemon -r dotenv/config app.ts dotenv_config_path=.env.pc",

    "migrate:postgres": "cross-env ENV_FILE=.env.postgres npx sequelize-cli db:migrate",
    "migrate:mssql": "cross-env ENV_FILE=.env.mssql npx sequelize-cli db:migrate",
    "migrate:pc": "cross-env ENV_FILE=.env.pc npx sequelize-cli db:migrate",
    
    "migrate:postgres:undo": "cross-env ENV_FILE=.env.postgres npx sequelize-cli db:migrate:undo",
    "migrate:mssql:undo": "cross-env ENV_FILE=.env.mssql npx sequelize-cli db:migrate:undo",
    "migrate:pc:undo": "cross-env ENV_FILE=.env.pc npx sequelize-cli db:migrate:undo",
    
    "migrate:postgres:undo:all": "cross-env ENV_FILE=.env.postgres npx sequelize-cli db:migrate:undo:all",
    "migrate:mssql:undo:all": "cross-env ENV_FILE=.env.mssql npx sequelize-cli db:migrate:undo:all",
    "migrate:pc:undo:all": "cross-env ENV_FILE=.env.pc npx sequelize-cli db:migrate:undo:all",
    
    "migrate:postgres:seed": "cross-env ENV_FILE=.env.postgres npx sequelize-cli db:seed",
    "migrate:mssql:seed": "cross-env ENV_FILE=.env.mssql npx sequelize-cli db:seed",
    "migrate:pc:seed": "cross-env ENV_FILE=.env.pc npx sequelize-cli db:seed",
    
    "migrate:postgres:seed:undo": "cross-env ENV_FILE=.env.postgres npx sequelize-cli db:seed:undo",
    "migrate:mssql:seed:undo": "cross-env ENV_FILE=.env.mssql npx sequelize-cli db:seed:undo",
    "migrate:pc:seed:undo": "cross-env ENV_FILE=.env.pc npx sequelize-cli db:seed:undo",
    
    "migrate:postgres:seed:all": "cross-env ENV_FILE=.env.postgres npx sequelize-cli db:seed:all",
    "migrate:mssql:seed:all": "cross-env ENV_FILE=.env.mssql npx sequelize-cli db:seed:all",
    "migrate:pc:seed:all": "cross-env ENV_FILE=.env.pc npx sequelize-cli db:seed:all",
    
    "migrate:postgres:seed:undo:all": "cross-env ENV_FILE=.env.postgres npx sequelize-cli db:seed:undo:all",
    "migrate:mssql:seed:undo:all": "cross-env ENV_FILE=.env.mssql npx sequelize-cli db:seed:undo:all",
    "migrate:pc:seed:undo:all": "cross-env ENV_FILE=.env.pc npx sequelize-cli db:seed:undo:all"

  },
  "keywords": [],
